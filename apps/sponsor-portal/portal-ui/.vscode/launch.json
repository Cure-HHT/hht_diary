{
  // Launch configurations for Portal UI flavors
  // See lib/flavors.dart for flavor details
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Portal Local",
      "type": "dart",
      "request": "launch",
      "program": "lib/main.dart",
      "args": [
        "-d", "chrome",
        "--dart-define=APP_FLAVOR=local",
        "--dart-define=FIREBASE_AUTH_EMULATOR_HOST=localhost:9099"
      ]
    },
    {
      "name": "Portal Dev",
      "type": "dart",
      "request": "launch",
      "program": "lib/main.dart",
      "args": [
        "-d", "chrome",
        "--dart-define=APP_FLAVOR=dev",
        "--dart-define=PORTAL_DEV_FIREBASE_API_KEY=${env:PORTAL_DEV_FIREBASE_API_KEY}",
        "--dart-define=PORTAL_DEV_FIREBASE_APP_ID=${env:PORTAL_DEV_FIREBASE_APP_ID}",
        "--dart-define=PORTAL_DEV_FIREBASE_PROJECT_ID=${env:PORTAL_DEV_FIREBASE_PROJECT_ID}",
        "--dart-define=PORTAL_DEV_FIREBASE_AUTH_DOMAIN=${env:PORTAL_DEV_FIREBASE_AUTH_DOMAIN}"
      ]
    },
    {
      "name": "Portal QA",
      "type": "dart",
      "request": "launch",
      "program": "lib/main.dart",
      "args": [
        "-d", "chrome",
        "--dart-define=APP_FLAVOR=qa",
        "--dart-define=PORTAL_QA_FIREBASE_API_KEY=${env:PORTAL_QA_FIREBASE_API_KEY}",
        "--dart-define=PORTAL_QA_FIREBASE_APP_ID=${env:PORTAL_QA_FIREBASE_APP_ID}",
        "--dart-define=PORTAL_QA_FIREBASE_PROJECT_ID=${env:PORTAL_QA_FIREBASE_PROJECT_ID}",
        "--dart-define=PORTAL_QA_FIREBASE_AUTH_DOMAIN=${env:PORTAL_QA_FIREBASE_AUTH_DOMAIN}"
      ]
    },
    {
      "name": "Portal UAT",
      "type": "dart",
      "request": "launch",
      "program": "lib/main.dart",
      "args": [
        "-d", "chrome",
        "--dart-define=APP_FLAVOR=uat",
        "--dart-define=PORTAL_UAT_FIREBASE_API_KEY=${env:PORTAL_UAT_FIREBASE_API_KEY}",
        "--dart-define=PORTAL_UAT_FIREBASE_APP_ID=${env:PORTAL_UAT_FIREBASE_APP_ID}",
        "--dart-define=PORTAL_UAT_FIREBASE_PROJECT_ID=${env:PORTAL_UAT_FIREBASE_PROJECT_ID}",
        "--dart-define=PORTAL_UAT_FIREBASE_AUTH_DOMAIN=${env:PORTAL_UAT_FIREBASE_AUTH_DOMAIN}"
      ]
    },
    {
      "name": "Portal Prod",
      "type": "dart",
      "request": "launch",
      "program": "lib/main.dart",
      "args": [
        "-d", "chrome",
        "--dart-define=APP_FLAVOR=prod",
        "--dart-define=PORTAL_PROD_FIREBASE_API_KEY=${env:PORTAL_PROD_FIREBASE_API_KEY}",
        "--dart-define=PORTAL_PROD_FIREBASE_APP_ID=${env:PORTAL_PROD_FIREBASE_APP_ID}",
        "--dart-define=PORTAL_PROD_FIREBASE_PROJECT_ID=${env:PORTAL_PROD_FIREBASE_PROJECT_ID}",
        "--dart-define=PORTAL_PROD_FIREBASE_AUTH_DOMAIN=${env:PORTAL_PROD_FIREBASE_AUTH_DOMAIN}"
      ]
    }
  ]
}
